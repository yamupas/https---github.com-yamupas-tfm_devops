import { EventEmitter, OnInit } from '@angular/core';
export declare class SwatchComponent implements OnInit {
    color: any;
    style: {
        [key: string]: string;
    };
    focusStyle: {
        [key: string]: string;
    };
    focus: boolean;
    onClick: EventEmitter<{
        hex: string;
        $event: Event;
    }>;
    onHover: EventEmitter<{
        hex: string;
        $event: Event;
    }>;
    divStyles: {
        [key: string]: string;
    };
    focusStyles: {
        [key: string]: string;
    };
    inFocus: boolean;
    ngOnInit(): void;
    currentStyles(): {
        [key: string]: string;
    };
    handleFocusOut(): void;
    handleFocus(): void;
    handleHover(hex: any, $event: any): void;
    handleClick(hex: any, $event: any): void;
}
export declare class SwatchModule {
}
